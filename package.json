{
  "name": "stylelint-config-growcss",
  "version": "1.0.0",
  "description": "A mostly reasonable approach to CSS.",
  "main": "index.js",
  "engines": {
    "node": "^9.0"
  },
  "scripts": {
    "test": "yarn run --silent tests-only",
    "tests-only": "ava ./test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growcss/stylelint-config-growcss.git"
  },
  "keywords": [
    "styleguide",
    "growcss",
    "css",
    "lint",
    "stylelint"
  ],
  "author": {
    "name": "Daniel Bannert",
    "email": "d.bannert@anolilab.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/growcss/stylelint-config-growcss/issues"
  },
  "homepage": "https://growcss.com",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "react": "^16.2.0",
    "styled-components": "^3.1.6",
    "stylelint": "^9.1.1"
  },
  "peerDependencies": {
    "stylelint": ">=9"
  },
  "dependencies": {
    "stylelint-config-standard": "^18.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^0.8.1",
    "stylelint-processor-styled-components": "^1.2.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4",
      "react"
    ]
  }
}
